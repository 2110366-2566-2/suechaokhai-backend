// .vscode/launch.json
{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Docker: Attach to Backend",
        "type": "go",
        "request": "attach",
        "mode": "remote",
        "remotePath": "/app",
        "port": 2345,
        "host": "127.0.0.1",
        "showLog": true,
        "trace": "verbose",
        "apiVersion": 2,
        "dlvToolPath": "${env.GOPATH}/bin/dlv"
      }
    ]
  }
  